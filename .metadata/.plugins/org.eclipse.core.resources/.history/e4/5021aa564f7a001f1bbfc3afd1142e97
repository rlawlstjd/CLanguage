import java.util.concurrent.ConcurrentLinkedQueue;

public class Test {
	public static void main(String[] args) {
		System.out.printf("5/2=" + 5/2);
		System.out.printf("5/ 2f="+ 5/2f); 
		System.out.println("5/2.0=" + 5/2.0);
		System.out.println("5 / (double)2 =" + 5/(double)2);
		
		System.out.println(1000000*1000000000000000l);
		System.out.println(100000*100000l);
		System.out.println(1000000L);
	
	
	}
}