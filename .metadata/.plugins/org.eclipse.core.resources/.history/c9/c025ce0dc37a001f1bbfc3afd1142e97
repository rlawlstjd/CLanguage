public class Test {
	public static void main(String[] args) {
		int arr [][] = new int[3][2];
		arr[0][0] = 70 ; 
		arr[0][1] = 80 ; 
		arr[1][0] = 30 ;
		arr[1][1] = 90 ; 
		arr[2][0] = 100 ; 
		arr[2][1] = 20 ;
		
		System.out.println("학새별 국어, 영어 점수");
		System.out.println("영희: " + arr[0][0] + ", " + arr[0][1] + "\n");
		System.out.println("철수: " + arr[1][0] + ", " + arr[1][1] + "\n");
		System.out.println("둘리: " + arr[2][0] + ", " + arr[2][1] + "\n"); 
		
		System.out.println(); 
		
		int[][] arr2 = {
				{80,95,90},
