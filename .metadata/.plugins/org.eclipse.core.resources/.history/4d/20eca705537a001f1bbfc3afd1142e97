public class Test {
	public static void main(String[] args) {
		 double d = 3.14; 
		 System.out.println("double d =" +d); 
		 
		 float f = 3.14f; 
		 System.out.println("float f =" + f); 
		 float ff = (float)3.14; 
		 System.out.println("float ff = " + ff); 
		 
		 double d2 = 7.21f; 
		 System.out.println("doubld d2 = " + d2); 
		 
		 int i = 5; 
		 int i2 = 2; 
		 System.out.println(i + "/" + i2 + "=" + i/i2); 
		 System.out.println(i + "/" + i2 + "=" + i/(float)i2); 
		 System.out.println(i + "/" + i2 + "=" + i/(double)i2); 
		 
		 float fff = 3.14f; 
		 int ii = (int)fff; 
		 System.out.println("int ii = "+ ii); 
		 
		 int i3 = 7; 
		 long l = i3; 
		 System.out.println("long은 int를 저장할 수 있습니다.");
		 System.out.println("int는 long을 저장할 수 없습니다.");
		 
		 long l2 = 1000000000000L; 
		 System.out.println("long 12= " +l2);
		 
		 
		 float f2 = l2; 
		 System.out.printf("float f2 = %f%n", l2); 
		 double d3 = l2;
		 System.out.println("doule f2= %f%n", d3); 
		 
		 String name = "둘리"; 
		 int age = 20 ; 
		 char bloodType = 'a';
		 float height = 203.7f; 
		 System.out.println(name + "의 나이는 " + age + "살 이고 혈액형은" + bloodType  );
		
	}
}