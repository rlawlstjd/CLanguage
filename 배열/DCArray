#include <stdio.h>
#include <string.h>

int main(void){
    char s[5][20] = {// 배열 초기화 
        {'d','o', 'g', '\0'}, 
        {'t', 'i', 'g', 'e', 'r', '\0'}
    }; 

    for (int i=0; i<5; i++){
        scanf("%s", s[i]); 
    }

    for (int i=0; i<5; i++){
        printf("%s", s[i]); 
    }

    // 배열 초기화 

    strcpy(s[0], "cat"); 
    
}